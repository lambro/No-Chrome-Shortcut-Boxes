{
  "manifest_version": 2,

  "name": "No Shortcuts",
  "description": "Remove annoying shortcut icons from home tab",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["noshortcut.js"]
    }
  ],
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
